<p id="notice"><%= notice %></p>

<h1>Listado de Compromisos</h1>

<table class="table table-striped" >
  <thead>
    <tr >
      <th>Dirigente</th>
      <th>Socio</th>
      <th>Compromiso</th>
      <th>Completado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.dirigente %></td>
        <td><%= list.socio.name %></td>
        <td><%= list.description %></td>
        <td><%= list.completed %></td>
        <td><% link_to 'Show', list %></td>
        <td><%= link_to 'Estatus', edit_list_path(list) %></td>
        <td><%= link_to 'Borrar', list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Compromiso', new_list_path, class:"btn btn-primary" %>
